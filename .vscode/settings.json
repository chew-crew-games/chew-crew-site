{
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "eslint.validate": ["vue", "javascript", "typescript"],
  "prettier.trailingComma": "es5",
  "prettier.arrowParens": "always",
  "files.exclude": {
    ".nuxt": true,
    ".output": true,
    "dist": true
  }
}
